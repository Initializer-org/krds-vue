{
  "name": "@krds.ui/vue",
  "version": "0.0.6",
  "description": "KRDS Vue Design System - 한국 정부 디자인 시스템 Vue 컴포넌트 라이브러리",
  "keywords": [
    "krds",
    "design-system",
    "vue",
    "vue3",
    "components",
    "ui-library",
    "typescript",
    "korea",
    "government"
  ],
  "homepage": "https://krds.initializer.org",
  "bugs": {
    "url": "https://github.com/Initializer-org/krds-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Initializer-org/krds-vue.git"
  },
  "license": "MIT",
  "author": "Initializer Team",
  "type": "module",
  "main": "./dist/krds-vue.umd.js",
  "module": "./dist/krds-vue.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/krds-vue.es.js",
      "require": "./dist/krds-vue.umd.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./style": "./dist/style.css",
    "./styles/*": "./dist/styles/*",
    "./styles": {
      "types": "./dist/types/styles/index.d.ts",
      "import": "./dist/styles/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run type-check && pnpm run build:lib",
    "build:lib": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "bundle-size": "vite build && echo 'Bundle size check removed due to bundlewatch security vulnerability'",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@rushstack/eslint-patch": "^1.12.0",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/vue3-vite": "^9.1.5",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.35.0",
    "eslint-plugin-storybook": "^9.1.5",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "sass": "^1.92.0",
    "source-map-explorer": "^2.5.3",
    "standard-version": "^9.5.0",
    "storybook": "^9.1.5",
    "typescript": "~5.7.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.2"
}
